{
	"name": "codekandis/radio-1-glasnost",
	"description": "`codekandis/radio-1-glasnost` is a CLI, API and HTML application to control an `Icecast 2` instance and an `Ezstream` source client.",
	"keywords": [
		"php",
		"radio-1-glasnost",
		"cli",
		"api",
		"html",
		"application",
		"icecast",
		"ezstream",
		"manager"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/radio-1-glasnost/issues",
		"docs": "https://github.com/codekandis/radio-1-glasnost/README.md",
		"source": "https://github.com/codekandis/radio-1-glasnost/tree/master"
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Radio1Glasnost\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Radio1Glasnost\\Tests\\": [
				"tests/"
			]
		}
	}
}
